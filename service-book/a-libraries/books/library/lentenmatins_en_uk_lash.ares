p0383 = "Through the protection of the Bodiless Powers, and save me."
p0384 = "On Tuesday"
p0385 = "Through the prayers of the Forerunner, and save me."
p0386 = "On Wednesday and Friday"
p0387 = "Through the power of your Cross, and save me."
p0388 = "On Thursday"
p0389 = "Through the prayers of the Apostles, and save me."
p0390 = "On Monday to Friday, the second time we end"
p0391 = "Through the prayers of the Apostle, and save me."
p0392 = "On Monday to Friday, the third time we end"
